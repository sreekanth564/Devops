pipeline {
    agent any
    
      options 
      {
      //To Ristrict the number of builds to be visible on jenkins
      // we don't fill up our storage!
      buildDiscarder(logRotator(numToKeepStr:'10'))
      
      //To Timeout
      timeout(time: 10, unit: 'MINUTES')            
      }
      
      
      
      stages
      {
      stage ('Checkout') 
      {
      steps {checkout scm }
      }
      stage ('Run Test Cases')
      {
      // condition should pass then only next step would run else it will skip but won't fail.
      when  { branch 'Feature_Feat1'}            
      steps { bat label: '', script: 'echo Hello From jenkins'} 
      }
      }
      }
      
